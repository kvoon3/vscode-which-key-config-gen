{
  "publisher": "kvoon",
  "name": "which-key-config-gen",
  "displayName": "Which key Config Generator",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.6.0",
  "description": "Generate vscode which-key config automatically",
  "author": "Kevin Kwong <kvoon@duck.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/kvoon3",
  "homepage": "https://github.com/kvoon3/vscode-which-key-config-gen#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/kvoon3/vscode-which-key-config-gen"
  },
  "bugs": {
    "url": "https://github.com/kvoon3/vscode-which-key-config-gen/issues"
  },
  "keywords": [
    "whichkey",
    "which-key",
    "keymap",
    "generete",
    "keybinding",
    "vim"
  ],
  "categories": [
    "Keymaps",
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "engines": {
    "vscode": "^1.90.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "title": "Which Key Config Generator: Show Menu",
        "command": "whichKeyConfigGen.show"
      },
      {
        "title": "Which Key Config Generator: Update Config",
        "command": "whichKeyConfigGen.updateConfig"
      },
      {
        "title": "Which Key Config Generator: Toggle Enable",
        "command": "whichKeyConfigGen.toggleEnable"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Which key Config Generator",
      "properties": {
        "whichKeyConfigGen.enable": {
          "type": "boolean",
          "default": true
        },
        "whichKeyConfigGen.bindings": {
          "type": "array",
          "items": "object",
          "default": []
        }
      }
    }
  },
  "sponsor": {
    "url": "https://github.com/sponsors/kvoon3"
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish",
    "update": "nlx vscode-ext-gen --output ./src/generated/meta.ts --scope whichKeyConfigGen",
    "up": "taze -I major",
    "postinstall": "npx simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.24.1",
    "@antfu/ni": "^0.22.0",
    "@antfu/utils": "^0.7.10",
    "@types/node": "^22.1.0",
    "@types/vscode": "^1.92.0",
    "@vscode/vsce": "^2.31.1",
    "bumpp": "^9.4.2",
    "eslint": "^9.8.0",
    "esno": "^4.7.0",
    "fast-glob": "^3.3.2",
    "lint-staged": "^15.2.8",
    "pnpm": "^9.6.0",
    "reactive-vscode": "0.2.0-beta.9",
    "rimraf": "^5.0.10",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.16.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vitest": "^2.0.5",
    "vscode": "^1.1.37"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
